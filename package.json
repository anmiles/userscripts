{
	"name": "@anmiles/userscripts",
	"version": "4.0.0",
	"description": "Userscripts for browsers",
	"keywords": [
		"userscripts",
		"parser",
		"customization",
		"maps"
	],
	"private": true,
	"author": "Anatoliy Oblaukhov",
	"homepage": "https://github.com/anmiles/userscripts",
	"repository": "github:anmiles/userscripts",
	"license": "MIT",
	"engines": {
		"node": ">=20.19.0"
	},
	"scripts": {
		"spellcheck": "cspell .",
		"prebuild": "rimraf dist",
		"build": "tsc -p ./tsconfig.build.json",
		"postbuild": "npm-run-all postbuild:*",
		"postbuild:prepare": "node scripts/prepare.js",
		"postbuild:minify": "node scripts/minify.js",
		"postbuild:copy": "copyfiles -u 1 dist/* public/",
		"postbuild:userscripts": "node ./scripts/userscripts.js",
		"lint": "eslint",
		"lint:fix": "eslint --fix"
	},
	"dependencies": {
		"@anmiles/prototypes": "^11.2.5",
		"copyfiles": "^2.4.1",
		"npm-run-all": "^4.1.5",
		"terser": "^5.46.0"
	},
	"devDependencies": {
		"@anmiles/eslint-config": "^10.0.3",
		"@anmiles/tsconfig": "^4.0.1",
		"@cspell/dict-ru_ru": "^2.3.2",
		"@eslint/compat": "^2.0.2",
		"@eslint/css": "^0.14.1",
		"@eslint/js": "^9.39.2",
		"@eslint/json": "^1.0.0",
		"@eslint/markdown": "^7.5.1",
		"@stylistic/eslint-plugin": "^5.7.1",
		"@types/jquery": "^3.5.33",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"cspell": "^9.6.2",
		"eslint": "^9.39.2",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-align-assignments": "^1.1.2",
		"eslint-plugin-i18next": "^6.1.3",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-n": "^17.23.2",
		"eslint-plugin-promise": "^7.2.1",
		"jiti": "^2.6.1",
		"jquery": "^4.0.0",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3"
	}
}
